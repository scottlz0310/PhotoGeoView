{
  "name": "photogeoview",
  "version": "2.1.8",
  "description": "ðŸ“ å†™çœŸã«åŸ‹ã‚è¾¼ã¾ã‚ŒãŸä½ç½®æƒ…å ±ã‚’åœ°å›³ä¸Šã«è¦‹ãˆã‚‹åŒ–ã™ã‚‹ã‚¹ã‚¿ãƒ³ãƒ‰ã‚¢ãƒ­ãƒ³ã‚¢ãƒ—ãƒªã€‚Electronãƒ»TypeScriptãƒ»Reactãƒ™ãƒ¼ã‚¹ã§ã€Exifãƒ‡ãƒ¼ã‚¿ã‚’è§£æžã—åœ°å›³ä¸Šã«ãƒ—ãƒ­ãƒƒãƒˆã€‚",
  "main": "./out/main/index.js",
  "author": "PhotoGeoView Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottlz0310/PhotoGeoView.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env ELECTRON_DISABLE_SANDBOX=1 electron-vite dev",
    "dev:wsl": "cross-env ELECTRON_DISABLE_SANDBOX=1 electron-vite dev --host",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "package": "electron-builder",
    "prepare": "lefthook install"
  },
  "build": {
    "appId": "com.photogeoview.app",
    "productName": "PhotoGeoView",
    "asarUnpack": [
      "node_modules/sharp/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*"
    ],
    "fileAssociations": [
      {
        "ext": [
          "jpg",
          "jpeg"
        ],
        "name": "JPEG Image",
        "description": "JPEG Image File",
        "mimeType": "image/jpeg",
        "role": "Viewer"
      },
      {
        "ext": "png",
        "name": "PNG Image",
        "description": "PNG Image File",
        "mimeType": "image/png",
        "role": "Viewer"
      },
      {
        "ext": [
          "tif",
          "tiff"
        ],
        "name": "TIFF Image",
        "description": "TIFF Image File",
        "mimeType": "image/tiff",
        "role": "Viewer"
      },
      {
        "ext": "webp",
        "name": "WebP Image",
        "description": "WebP Image File",
        "mimeType": "image/webp",
        "role": "Viewer"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "fileAssociations": []
    },
    "publish": [
      {
        "provider": "github",
        "owner": "scottlz0310",
        "repo": "PhotoGeoView",
        "releaseType": "release"
      }
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.6.2",
    "exifreader": "^4.32.0",
    "i18next": "^25.6.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.554.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^16.3.5",
    "react-leaflet": "^5.0.0",
    "react-resizable-panels": "^3.0.6",
    "react-zoom-pan-pinch": "^3.7.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@evilmartians/lefthook": "^2.0.4",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "electron": "^39.2.1",
    "electron-builder": "24.13.3",
    "electron-vite": "^4.0.1",
    "happy-dom": "^20.0.10",
