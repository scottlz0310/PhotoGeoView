{
  "app": {
    "name": "PhotoGeoView",
    "version": "1.0.0",
    "debug_mode": false,
    "auto_save": true,
    "backup_enabled": true,
    "telemetry_enabled": false
  },
  "ui": {
    "theme": "default",
    "thumbnail_size": 150,
    "window_geometry": null,
    "splitter_states": {},
    "show_toolbar": true,
    "show_statusbar": true,
    "animation_enabled": true
  },
  "core": {
    "image_formats": [
      ".jpg",
      ".jpeg",
      ".png",
      ".tiff",
      ".bmp",
      ".gif"
    ],
    "exif_parsing_enabled": true,
    "map_provider": "folium",
    "map_zoom_default": 10,
    "thumbnail_quality": 85,
    "cache_enabled": true,
    "cache_size_mb": 100
  },
  "performance": {
    "mode": "balanced",
    "max_memory_mb": 512,
    "thread_pool_size": 4,
    "async_loading": true,
    "preload_thumbnails": true,
    "performance_monitoring": true
  },
  "logging": {
    "level": "INFO",
    "file_logging": true,
    "console_logging": true,
    "performance_logging": true,
    "ai_operation_logging": true,
    "max_log_size_mb": 10,
    "log_retention_days": 30
  }
}
